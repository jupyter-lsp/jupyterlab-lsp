{
  "@jupyter-lsp/jupyterlab-lsp:plugin": {
    "language_servers": {
      "robotframework_ls": {
        "serverSettings": {
          "robot.lint.robocop.enabled": true
        }
      },
      "texlab": {
        "serverSettings": {
          "chktex.onOpenAndSave": true,
          "chktex.onEdit": true
        }
      },
      "pyright": {
        "serverSettings": {
          "python.analysis.useLibraryCodeForTypes": true
        }
      }
    }
  }
}
