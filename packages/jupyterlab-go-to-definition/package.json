    "@jupyterlab/services": "5",
    "@lumino/algorithm": "*",
    "codemirror": "*",
    "react": "*",
    "@jupyterlab/testutils": "2",
    "@types/codemirror": "^0.0.74",
    "@types/chai": "^4.1.7",
    "@types/jest": "^23.3.11",
    "chai": "^4.2.0",
    "jest": "^24.7.1",
    "jest-junit": "^8.0.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^24.0.2",
    "typescript": "~3.9.3",
    "@babel/preset-env": "^7.4.3"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
